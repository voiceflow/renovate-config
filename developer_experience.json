{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "A base Renovate preset for the DX team",
  "extends": [
    "github>voiceflow/renovate-config"
  ],
  "reviewers": [
    "effervescentia"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "dockerfile",
        "docker-compose"
      ],
      "matchPackageNames": [
        "node"
      ],
      "versioning": "node",
      "enabled": true
    }
  ]
}